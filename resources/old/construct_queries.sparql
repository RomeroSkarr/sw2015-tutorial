#################################################################
#
#    EventsThatHappenedInFrance.ttl - WON'T BE NEEDING THIS ONE
#
#################################################################

PREFIX dbr: <http://dbpedia.org/resource/>
PREFIX dbo: <http://dbpedia.org/ontology/>

CONSTRUCT {  
  ?societalEvent dbo:place dbr:France .
  ?societalEvent a dbo:SocietalEvent .
} WHERE {
  ?societalEvent dbo:place dbr:France .
  ?societalEvent a dbo:SocietalEvent .
}


